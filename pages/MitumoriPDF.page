    <style type="text/css" media="print">
    @page :first {
        size: 21.0cm 29.7cm;
        margin-top: 7.9cm;
        margin-left: 1.0cm;
        margin-right: 1.0cm;
        margin-bottom: 0.9cm;
        @bottom-center {
            content: element(footer);
        }
        @top-center {
            content: element(head1);
        }
    }

    @page {
        size: 21.0cm 29.7cm;
        margin-top: 7.2cm;
        margin-left: 1.0cm;
        margin-right: 1.0cm;
        margin-bottom: 0.9cm;

        @bottom-center {
            content: element(footer);
        }

        @top-center {
            content: element(head2);
        }

        thead {
            display: table-header-group;
        }


    }

    div.head1{
        position: running(head1);
    }

    div.head2{
        position: running(head2);
    }

    div.footer {
        display: block;
        font-size: 11px;
        
        padding-bottom: 15px;
        text-align: right;
        position: running(footer);
    }

    .pagenumber:before {
        content: counter(page);
    }

    .pagecount:before {
        content: counter(pages);
    }
    </style>
