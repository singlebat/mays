            padding-right: 2px
        }

        .textleft {
            text-align: left;
            padding-left: 2px
        }

    

        .page-break {
            page-break-before: always;
            width: 18cm;
            height: 20cm;
            margin-left: 1.5cm;
        }

        .page-break-after {
            page-break-after: always;
        }

        .page-break-inside {
            page-break-inside: avoid;
        }


         .titleposition {
            position: absolute;
        }

        .logoposition {
            position: absolute;
            right: 0px;
            top: 110px;
        }


        .titlebango{
            position: absolute;
            top: 1.1cm;
        }

        .divsty{float:right;border-bottom: solid 0.5px #000}


